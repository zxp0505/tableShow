<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="40dp"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/tv_goodBar"
        style="@style/tv_good_detail" />

    <TextView
        android:id="@+id/tv_goodClasses"
        style="@style/tv_good_detail"
        />

    <TextView
        android:id="@+id/tv_goodStandard"
        style="@style/tv_good_detail" />

    <TextView
        android:id="@+id/tv_goodUnit"
        style="@style/tv_good_detail" />

    <TextView
        android:id="@+id/tv_goodSupplier"
        style="@style/tv_good_detail" />

    <TextView
        android:id="@+id/tv_goodPrice"
        style="@style/tv_good_detail"/>

    <TextView
        android:id="@+id/tv_goodQuality"
        style="@style/tv_good_detail" />

    <TextView
        android:id="@+id/tv_goodPurchaseDate"
        style="@style/tv_good_detail" />

    <TextView
        android:id="@+id/tv_goodBuyPrice"
        style="@style/tv_good_detail" />
</LinearLayout>